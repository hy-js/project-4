<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <%- include('../partials/header.ejs') %>
    <h1>All Users</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>User</th>
          <th>Title</th>
          <th>Content</th>
          <th>Created</th>
        </tr>
      </thead>
      <tbody>
        <% if(posts.length > 0) { %>
          <% posts.forEach(element => { %>
            <!-- Print all -->
            <tr>
            <% for (const property in element) { %>
                <td>
                  <%= element[property] %>
                </td>
                <% } %>
              </tr>
            <%  }) %>
        <% }  %>
      </tbody>
    </table>
  </body>
</html>
